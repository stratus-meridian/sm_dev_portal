  "conflict": {
    "drupal/pathauto": "<1.6"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "enable-patching": true,
    "patches": {
      "drupal/core": {
        "Support entities that are neither content nor config entities": "https://www.drupal.org/files/issues/2020-12-02/3042467-50.patch"
      }
    }
  }
}
