    "drupal/sm_appdashboard_apigee": "^1.2",
    "drupal/smart_trim": "^1.2",
    "drupal/svg_image": "^1.13",
    "drupal/swagger_ui_formatter": "^3.3",
    "drupal/term_reference_tree": "^1.0@alpha",
    "drupal/token": "^1.6",
